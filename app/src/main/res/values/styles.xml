<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:textSize">@dimen/common_size</item>
    </style>
    <style name="CallWindow">
        <item name="android:windowEnterAnimation">@anim/anim_call_out</item>
        <item name="android:windowExitAnimation">@anim/anim_call_back</item>
    </style>
    <style name="PayCheckBoxTheme" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/checkbox_method</item>
    </style>
</resources>
